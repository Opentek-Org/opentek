{
  "$schema": "http://json.schemastore.org/web-manifest",
  "short_name": "Opentek",
  "name": "Opentek",
  "description": "If it is about Open Source, it is on Opentek.",
  "scope": "/",
  "icons": [
    {
      "src": "icons/manifest-icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "icons/manifest-icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable any"
    }
  ],
  "background_color": "#080B11",
  "theme_color": "#080B11",
  "start_url": "./index.html?from=homescreen",
  "display": "standalone",
  "orientation": "any"
}
